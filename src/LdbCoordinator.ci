extern message InitMsg;
extern message DoneMsg;
extern message QuiescenceMessage;
packmessage LdbStatsMsg;
message LdbResumeMsg;

group LdbCoordinator
{
  entry LdbCoordinator(InitMsg *);
  entry nodeDone(LdbResumeMsg *);
  entry sendStats(LdbResumeMsg *);
  entry analyze(LdbStatsMsg *);
  entry resume(LdbResumeMsg *);
  entry resumeReady(QuiescenceMessage *);
  entry resume2(LdbResumeMsg *);
  entry updateComputesReady(DoneMsg *);
} ;


