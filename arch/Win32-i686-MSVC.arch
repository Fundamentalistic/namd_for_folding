NAMD_ARCH = Win32-i686
CHARMARCH = net-win32

CXX = cl /nologo /MT /TP /DWIN32 /DSOCKLEN_T=int /D_AIX /D_CONSOLE /Zp4
CC = cl /nologo /MT /DWIN32 /D_CONSOLE
LINK = link /nologo /subsystem:console /incremental:no \
  kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib \
  advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib \
  odbccp32.lib ws2_32.lib

CXXOPTS = /O2 /G6
CXXTHREADOPTS = /G6
COPTS = /O2 /G6
LINKOPTS = /OPT:REF

winall: win32binaries

