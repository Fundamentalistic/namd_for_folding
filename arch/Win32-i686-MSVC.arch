NAMD_ARCH = Win32
CHARMARCH = net-win32

CXX = cl /nologo /MT /TP /D "WIN32" /D "_AIX" /D "_CONSOLE" /D "DUMMY_VMDSOCK"
LINK = link /nologo /subsystem:console /incremental:no /machine:I386 \
  kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib \
  advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib \
  odbccp32.lib ws2_32.lib

CXXOPTS = 
CXXTHREADOPTS = $(CXXOPTS)
CXXSIMPARAMOPTS = $(CXXOPTS)
LINKOPTS = /OPT:REF

winall: win32binaries

