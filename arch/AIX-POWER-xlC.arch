NAMD_ARCH = AIX-POWER
CHARMARCH = net-rs6k

CXX = xlC_r -w -qstaticinline
CXXOPTS = -O3 -qstrict -qaggrcopy=nooverlap -qalias=addr:ansi:typ -Q -qarch=com -qtune=pwr3 -qfloat=rsqrt:fltint -bmaxdata:0x80000000
CXXNOALIASOPTS = -O3 -qaggrcopy=nooverlap -qalias=addr:allp:ansi:typ -Q -qarch=com -qtune=pwr3 -qfloat=rsqrt:fltint -bmaxdata:0x80000000
CC = xlc_r -w
COPTS = -O4 -qinlglue -qarch=com -qtune=pwr3 -bmaxdata:0x80000000

