NAMD_ARCH = Linux-x86
CHARMARCH = net-linux-iccstatic

FLOATOPTS = -march=pentiumpro -mcpu=pentium4 -ip -fno-rtti

CXX = icpc
CXXOPTS = -i-static -static-libcxa -O2 $(FLOATOPTS)
CXXNOALIASOPTS = -O3 -fno-alias $(FLOATOPTS) -DSIMPLE_PAIRLIST

CC = icc
COPTS = -i-static -static-libcxa -O2 $(FLOATOPTS)


